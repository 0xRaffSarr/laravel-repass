{
    "name": "xraffsarr/laravel-repass",
    "description": "A custom password reset for Laravel",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/0xRaffSarr/laravel-repass",
    "version": "0.0.2",
    "autoload": {
        "psr-4": {
            "Xraffsarr\\LaravelRePass\\": "src/"
        }
    },
    "authors": [
        {
            "role": "developer",
            "name": "Raffaele Sarracino",
            "email": "work@raffaelesarracino.it",
            "homepage": "https://raffaelesarracino.it"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/auth": "^11.34",
        "illuminate/support": "^9.21|^10.0|^11.0"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xraffsarr\\LaravelRePass\\RePassServiceProvider"
            ],
            "aliases": {
                "RePass": "Xraffsarr\\LaravelRePass\\Facade\\RePass"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
